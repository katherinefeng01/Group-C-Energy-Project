   "source": [
    "# raw data\n",
    "raw = pd.read_csv('owid-energy-data.csv')\n",
    "\n",
    "df = raw[raw['year']>1969]\n",
    "\n",
    "#A list of all of the countries included and all of the features \n",
    "countries = list(df['country'].unique())\n",
    "all_features = list(df)"
   ]
